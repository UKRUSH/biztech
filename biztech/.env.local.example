# Local Environment Variables Example
# Copy this file to .env.local and fill in your actual values

# Email Service Keys (Required for contact form)
VITE_EMAILJS_SERVICE_ID=your_emailjs_service_id
VITE_EMAILJS_TEMPLATE_ID=your_emailjs_template_id  
VITE_EMAILJS_PUBLIC_KEY=your_emailjs_public_key
VITE_WEB3FORMS_ACCESS_KEY=your_web3forms_access_key
VITE_GETFORM_ENDPOINT=https://getform.io/f/your_form_id

# Analytics Keys (Optional)
VITE_GOOGLE_ANALYTICS_ID=GA-XXXX-XXXXX
VITE_FACEBOOK_PIXEL_ID=your_facebook_pixel_id
VITE_HOTJAR_ID=your_hotjar_id

# Custom API Endpoints (Optional)
VITE_CUSTOM_API_URL=https://your-custom-api.com
VITE_WEBHOOK_URL=https://your-webhook-endpoint.com

# Local Development Overrides
VITE_LOCAL_PORT=3001
VITE_LOCAL_HOST=127.0.0.1